{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "system-matugen",
  "author": "pywal",
  "themes": [
    {
      "name": "system-matugen",
      "appearance": "dark",
      "style": {
        "background": "{{colors.background.default.hex}}",
        "editor.background": "{{colors.background.default.hex}}",
        "editor.foreground": "{{colors.on_background.default.hex}}",
        "text": "{{colors.on_background.default.hex}}",
        "text.muted": "{{colors.on_surface_variant.default.hex}}",
        "text.ignored": "{{colors.outline.default.hex}}",
        "text.placeholder": "{{colors.on_surface_variant.default.hex}}",
        "ignored": "{{colors.outline.default.hex}}",
        "element.hover": "{{colors.surface_container.default.hex}}",
        "ghost_element.hover": "{{colors.primary_container.default.hex}}",
        "ghost_element.selected": "{{colors.primary_container.default.hex}}",
        "ghost_element.active": "{{colors.primary.default.hex}}",
        "border": "{{colors.outline_variant.default.hex}}",
        "editor.highlighted_line.background": "{{colors.primary_container.default.hex}}",
        "editor.active_line.background": "{{colors.surface_container_high.default.hex}}",
        "panel.background": "{{colors.surface_container_low.default.hex}}",
        "title_bar.background": "{{colors.surface_container_lowest.default.hex}}",
        "title_bar.inactive_background": "{{colors.surface_container_lowest.default.hex}}",
        "status_bar.background": "{{colors.surface_container_lowest.default.hex}}",
        "drop_target.background": "{{colors.surface_container_high.default.hex}}",
        "elevated_surface.background": "{{colors.surface_container_highest.default.hex}}",
        "toolbar.background": "{{colors.surface_container_low.default.hex}}",
        "tab_bar.background": "{{colors.surface_container_lowest.default.hex}}",
        "tab.inactive_background": "{{colors.surface_container_lowest.default.hex}}",
        "tab.active_background": "{{colors.surface_container_high.default.hex}}",
        "scrollbar.track.background": "transparent",
        "scrollbar.track.border": "{{colors.outline_variant.default.hex}}",
        "scrollbar.thumb.background": "{{colors.on_surface_variant.default.hex}}",
        "editor.gutter.background": "{{colors.surface_container_low.default.hex}}",
        "terminal.background": "{{colors.background.default.hex}}",
        "terminal.foreground": "{{colors.on_background.default.hex}}",
        "terminal.dim_foreground": "{{colors.on_surface_variant.default.hex}}",
        "terminal.bright_foreground": "{{colors.on_background.default.hex}}",
        "terminal.ansi.black": "{{colors.primary.default.hex}}",
        "terminal.ansi.red": "{{colors.red.default.hex}}",
        "terminal.ansi.green": "{{colors.green.default.hex}}",
        "terminal.ansi.yellow": "{{colors.yellow.default.hex}}",
        "terminal.ansi.blue": "{{colors.blue.default.hex}}",
        "terminal.ansi.magenta": "{{colors.magenta.default.hex}}",
        "terminal.ansi.cyan": "{{colors.cyan.default.hex}}",
        "terminal.ansi.white": "{{colors.white.default.hex}}",
        "terminal.ansi.bright_black": "{{colors.outline.default.hex}}",
        "terminal.ansi.bright_red": "{{colors.error.default.hex}}",
        "terminal.ansi.bright_green": "{{colors.green_container.default.hex}}",
        "terminal.ansi.bright_yellow": "{{colors.yellow_container.default.hex}}",
        "terminal.ansi.bright_blue": "{{colors.blue_container.default.hex}}",
        "terminal.ansi.bright_magenta": "{{colors.magenta_container.default.hex}}",
        "terminal.ansi.bright_cyan": "{{colors.cyan_container.default.hex}}",
        "terminal.ansi.bright_white": "{{colors.white_container.default.hex}}",
        "modified": "{{colors.primary.default.hex}}",
        "syntax": {
          "attribute": {
            "color": "{{colors.primary.default.hex}}"
          },
          "boolean": {
            "color": "{{colors.tertiary.default.hex}}"
          },
          "comment": {
            "color": "{{colors.outline.default.hex}}"
          },
          "comment.doc": {
            "color": "{{colors.outline.default.hex}}"
          },
          "constant": {
            "color": "{{colors.secondary.default.hex}}"
          },
          "function": {
            "color": "{{colors.tertiary_fixed_dim.default.hex}}"
          },
          "keyword": {
            "color": "{{colors.primary_fixed_dim.default.hex}}"
          },
          "number": {
            "color": "{{colors.tertiary.default.hex}}"
          },
          "operator": {
            "color": "{{colors.primary_fixed_dim.default.hex}}"
          },
          "string": {
            "color": "{{colors.green.default.hex}}"
          },
          "variable": {
            "color": "{{colors.on_surface.default.hex}}"
          }
        },
        "players": [
          {
            "cursor": "{{colors.primary.default.hex}}",
            "background": "{{colors.background.default.hex}}",
            "selection": "{{colors.primary_container.default.hex}}"
          }
        ]
      }
    }
  ]
}
