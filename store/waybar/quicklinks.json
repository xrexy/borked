{
  "custom/quicklink1": {
    "format": " ",
    "on-click": "zed ~/.dotfiles",
    "tooltip-format": "Edit .dotfiles"
  },
  "custom/quicklink2": {
    "format": " ",
    "on-click": "((pkill -0 -x brave || pkill -0 -x .brave-wrapped) && hyprctl dispatch focuswindow class:Brave-browser) || brave",
    "on-click-right": "brave",
    "tooltip-format": "Open Brave"
  },
  "custom/quicklink3": {
    "format": " ",
    "on-click": "thunar ~",
    "on-click-right": "thunar /",
    "tooltip-format": "Open Thunar"
  },
  "custom/quicklinkempty": {},
  "group/quicklinks": {
    "orientation": "horizontal",
    "modules": [
      "custom/quicklink1",
      "custom/quicklink2",
      "custom/quicklink3"
      // "custom/quicklinkempty"
    ]
  }
}
